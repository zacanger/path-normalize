{
  "name": "path-normalize",
  "description": "Zero-dependency browser-oriented path.normalize",
  "version": "2.0.1",
  "author": {
    "name": "Zac Anger",
    "email": "zac@zacanger.com",
    "url": "http://zacanger.com"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib",
    "index.d.ts"
  ],
  "scripts": {
    "build": "babel src/index.js --out-file lib/index.js",
    "cover": "nyc npm run test:tape",
    "prebuild": "npm run test:lint && npm run cover",
    "preversion": "npm run build",
    "test": "npm run test:lint && npm run test:tape",
    "test:lint": "eslint -c .eslintrc.json index.js test.js",
    "test:tape": "babel-tape-runner src/test.js | tap-spec"
  },
  "homepage": "https://github.com/zacanger/path-normalize#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacanger/path-normalize.git"
  },
  "bugs": "https://github.com/zacanger/path-normalize/issues",
  "keywords": [
    "normalize",
    "path",
    "path-normalize"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-preset-env": "1.6.1",
    "babel-tape-runner": "2.0.1",
    "eslint": "4.19.1",
    "eslint-config-zacanger": "3.1.1",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-unicorn": "4.0.3",
    "nyc": "11.7.1",
    "tap-spec": "4.1.1",
    "tape": "4.9.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "nyc": {
    "include": [
      "index.js"
    ],
    "sourceMap": true,
    "instrument": true,
    "reporter": [
      "text",
      "html",
      "json"
    ]
  },
  "typings": "index.d.ts"
}
