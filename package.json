{
  "name": "path-normalize",
  "description": "Zero-dependency browser-oriented path.normalize",
  "version": "3.0.2",
  "author": {
    "name": "Zac Anger",
    "email": "zac@zacanger.com",
    "url": "http://zacanger.com"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib",
    "index.d.ts"
  ],
  "scripts": {
    "build": "babel src/index.js --out-file lib/index.js",
    "clean": "rm -rf lib && mkdir -p lib",
    "cover": "nyc npm run test:tape",
    "prebuild": "run-s -s test:lint cover clean",
    "preversion": "npm run build",
    "test": "run-s -s test:lint test:tape",
    "test:lint": "eslint src",
    "test:tape": "babel-tape-runner src/test.js"
  },
  "homepage": "https://github.com/zacanger/path-normalize#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacanger/path-normalize.git"
  },
  "bugs": "https://github.com/zacanger/path-normalize/issues",
  "keywords": [
    "normalize",
    "path",
    "path-normalize"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.6",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-env": "1.7.0",
    "babel-tape-runner": "2.0.1",
    "eslint": "5.3.0",
    "eslint-config-zacanger": "3.4.1",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-unicorn": "5.0.0",
    "npm-run-all": "4.1.3",
    "nyc": "12.0.2",
    "tape": "4.9.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "nyc": {
    "include": [
      "src/index.js"
    ],
    "sourceMap": true,
    "instrument": true,
    "reporter": [
      "text",
      "html",
      "json"
    ]
  },
  "typings": "index.d.ts"
}
